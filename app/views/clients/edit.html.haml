.col-md-8
  = form_for @client, html: {role: "form", class: "form-horizontal"} do |f|
    = render :partial => 'form', :locals => { :f => f }
    = f.submit "Update", class: "btn btn-primary btn-lg"
    = link_to "Delete",  {:action => "destroy", :id => @client.id }, :class => "btn btn-warning btn-lg", :role => "button", :method => "delete"
